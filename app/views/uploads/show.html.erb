
<div class="container">




				
                <div class="col-md-4 col-xs-6">
                    <div class="panel">
                    	<h1>Title: <%= @upload.title %> </h1> 
                    	<hr>
                    	<%= link_to 'Edit', edit_upload_path(@upload), class: "btn btn-warning" %> 
						<%= link_to 'Back', uploads_path, class: "btn btn-info" %>
